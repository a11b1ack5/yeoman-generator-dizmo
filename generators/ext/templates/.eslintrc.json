{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "assert": false,
    "dizmo": false,
    "viewer": false
  },
  "rules": {
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-console": [0]
  }
}
