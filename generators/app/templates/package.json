{
    "dizmo": {
        "install-to": "<%= installTo %>",
        "settings": {
            "bundle-identifier": "<%= bundleId %>",
            "height": 240, "width": 480
        }
    },
    "person": {
        "name": "<%= personName %>",
        "email": "<%= personEmail %>"
    },
    "description": "<%= dizmoDescription %>",
    "devDependencies": {
        "gulp": "^3.9.0",
        "gulp-concat": "^2.6.0",
        "gulp-copy": "^0.0.2",
        "gulp-plist": "^0.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-ver": "^0.1.0",
        "gulp-zip": "^3.0.2",
        "lodash": "^3.10.1",
        "require-dir": "^0.3.0",
        "rimraf": "^2.4.4"
    },
    "keywords": [
        "dizmo", "<%= _.snakeCase(dizmoName) %>"
    ],
    "license": "ISC",
    "name": "<%= dizmoName %>",
    "private": true,
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "clean": "node ./node_modules/gulp/bin/gulp.js clean",
        "install": "node ./node_modules/gulp/bin/gulp.js install",
        "lint": "node ./node_modules/gulp/bin/gulp.js lint",
        "make": "node ./node_modules/gulp/bin/gulp.js",
        "test": "exit 0",
        "upload": "node ./node_modules/gulp/bin/gulp.js upload",
        "watch": "node ./node_modules/gulp/bin/gulp.js watch"
    },
    "version": "0.0.0"
}
